			<dcif:administrationRecord>
				<dcif:identifier>lightVerb</dcif:identifier>
				<dcif:version>1:0</dcif:version>
				<dcif:registrationStatus>private</dcif:registrationStatus>
				<dcif:justification>missing in the categorisation of verbs</dcif:justification>
				<dcif:creation>
					<dcif:creationDate>2010-04-14</dcif:creationDate>
					<dcif:changeDescription xml:lang="en">creation</dcif:changeDescription>
				</dcif:creation>
				<dcif:lastChange>
					<dcif:lastChangeDate>2010-04-25</dcif:lastChangeDate>
					<dcif:changeDescription xml:lang="en">Urdu and Hindi examples.</dcif:changeDescription>
				</dcif:lastChange>
			</dcif:administrationRecord>
		</dcif:administrationInformationSection>
		<dcif:descriptionSection>
			<dcif:profile>Morphosyntax</dcif:profile>
			<dcif:dataElementNameSection>
				<dcif:dataElementName>light verb</dcif:dataElementName>
				<dcif:source>
					http://www.panl10n.net/Presentations/Laos/RegionalConference/CorpusCollection/Tagset_and_Tagging_Urdu_Corpus.pdf</dcif:source>
			</dcif:dataElementNameSection>
			<dcif:languageSection>
				<dcif:language>en</dcif:language>
				<dcif:nameSection>
					<dcif:name xml:lang="en">light verb</dcif:name>
					<dcif:nameStatus>standardized name</dcif:nameStatus>
				</dcif:nameSection>
				<dcif:nameSection>
					<dcif:name xml:lang="en">support verb</dcif:name>
					<dcif:nameStatus>admitted name</dcif:nameStatus>
				</dcif:nameSection>
				<dcif:definitionSection>
					<dcif:definition xml:lang="en">verb participating in a complex predication that
						has little semantic content of its own.</dcif:definition>
					<dcif:source>wikipedia</dcif:source>
					<dcif:note xml:lang="en" />
				</dcif:definitionSection>
				<dcif:exampleSection>
					<dcif:example xml:lang="en">en: "take" in "I take a drive"</dcif:example>
				</dcif:exampleSection>
				<dcif:exampleSection>
					<dcif:example xml:lang="en">ur: "کرنا" ("karnā") as in "صاف کرنا" ("şāf karnā")
						= lit. "clean make" = "clean"</dcif:example>
					<dcif:source>
						http://www.panl10n.net/Presentations/Laos/RegionalConference/CorpusCollection/Tagset_and_Tagging_Urdu_Corpus.pdf</dcif:source>
				</dcif:exampleSection>
				<dcif:exampleSection>
					<dcif:example xml:lang="en">hi: "शुरू करना" ("śurū karnā") = lit. "beginning
						make" = "to begin"; "ख़त्म करना" ("xatm karnā") = lit. "finished make" = "to
						finish"; "बंद करना" ("band karnā") = lit. "closed make" = "to shut"; "साफ़
						करना" ("sāf karnā") = lit. "clean make" = "to clean"</dcif:example>
					<dcif:source>Rupert Snell, Simon Weightman: Teach Yourself Hindi, Hodder
						Education, London, 2003, p. 93 (7.6)</dcif:source>
				</dcif:exampleSection>
				<dcif:explanationSection>
					<dcif:explanation xml:lang="en">A light verb behaves similarly to a copula. In
						Hindi, copula constructions form the intransitive counterparts to the
						transitive conjunct verbs: "वह कमरा साफ़ करता है" ("vah kamrā sāf kartā hai")
						= lit. "he room clean do is" = "he cleans the room" (transitive) vs. "कमरा
						साफ़ होता है" ("kamrā sāf hotā hai") = lit. "room clean be is" = "the room is
						clean" (intransitive).</dcif:explanation>
					<dcif:source>Rupert Snell, Simon Weightman: Teach Yourself Hindi, Hodder
						Education, London, 2003, p. 93 (7.6)</dcif:source>
				</dcif:explanationSection>
			</dcif:languageSection>
		</dcif:descriptionSection>
