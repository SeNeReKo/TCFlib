	<dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1230" type="simple">
		<dcif:administrationInformationSection>
			<dcif:administrationRecord>
				<dcif:identifier>adjective</dcif:identifier>
				<dcif:version>1:0</dcif:version>
				<dcif:registrationStatus>private</dcif:registrationStatus>
				<dcif:justification>ISO 12620 and others</dcif:justification>
				<dcif:origin>ISO 12620, morphosyntax group</dcif:origin>
				<dcif:effectiveDate>2012-06-08</dcif:effectiveDate>
				<dcif:creation>
					<dcif:creationDate>2004-07-09</dcif:creationDate>
					<dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
				</dcif:creation>
				<dcif:lastChange>
					<dcif:lastChangeDate>2012-06-16</dcif:lastChangeDate>
					<dcif:changeDescription xml:lang="en">Added article.</dcif:changeDescription>
				</dcif:lastChange>
			</dcif:administrationRecord>
		</dcif:administrationInformationSection>
		<dcif:descriptionSection>
			<dcif:profile>Morphosyntax</dcif:profile>
			<dcif:profile>Terminology</dcif:profile>
			<dcif:dataElementNameSection>
				<dcif:dataElementName>adjective</dcif:dataElementName>
				<dcif:source>ISO 12620; morphosyntax</dcif:source>
			</dcif:dataElementNameSection>
			<dcif:languageSection>
				<dcif:language>en</dcif:language>
				<dcif:nameSection>
					<dcif:name xml:lang="en">adjective</dcif:name>
					<dcif:nameStatus>standardized name</dcif:nameStatus>
				</dcif:nameSection>
				<dcif:definitionSection>
					<dcif:definition xml:lang="en">Part of speech related to attributes of a noun.</dcif:definition>
					<dcif:source>GF, morphosyntax </dcif:source>
					<dcif:note xml:lang="en">We speak of an adjective when one can ask the question:
						how is something?</dcif:note>
				</dcif:definitionSection>
				<dcif:exampleSection>
					<dcif:example xml:lang="en"> A big horse </dcif:example>
					<dcif:source> www.southwestern.edu/~carlg/Latin_Web/glossary.html </dcif:source>
				</dcif:exampleSection>
			</dcif:languageSection>
		</dcif:descriptionSection>
	</dcif:dataCategory>
	<dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1231" type="simple">
		<dcif:administrationInformationSection>
			<dcif:administrationRecord>
				<dcif:identifier>adposition</dcif:identifier>
				<dcif:version>1:0</dcif:version>
				<dcif:registrationStatus>private</dcif:registrationStatus>
				<dcif:justification>EAGLES basic POS</dcif:justification>
				<dcif:origin>Member of MAF DCS</dcif:origin>
				<dcif:effectiveDate>2012-06-08</dcif:effectiveDate>
				<dcif:creation>
					<dcif:creationDate>2004-07-09</dcif:creationDate>
					<dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
				</dcif:creation>
				<dcif:lastChange>
					<dcif:lastChangeDate>2012-06-16</dcif:lastChangeDate>
					<dcif:changeDescription xml:lang="en">Corrected verb form.</dcif:changeDescription>
				</dcif:lastChange>
			</dcif:administrationRecord>
		</dcif:administrationInformationSection>
		<dcif:descriptionSection>
			<dcif:profile>Morphosyntax</dcif:profile>
			<dcif:languageSection>
				<dcif:language>en</dcif:language>
				<dcif:nameSection>
					<dcif:name xml:lang="en">adposition</dcif:name>
					<dcif:nameStatus>standardized name</dcif:nameStatus>
				</dcif:nameSection>
				<dcif:definitionSection>
					<dcif:definition xml:lang="en">Part of speech that occurs before/inside/after a
						complement composed of a noun phrase, noun, pronoun or clause that functions
						as a noun phrase and forms a single structure with the complement to express
						its grammatical and semantic relation to another unit.</dcif:definition>
					<dcif:source>Eagles</dcif:source>
					<dcif:note xml:lang="en">Is a cover term for preposition, circumposition and
						postposition.</dcif:note>
				</dcif:definitionSection>
			</dcif:languageSection>
		</dcif:descriptionSection>
	</dcif:dataCategory>
	<dcif:dataCategory pid="http://www.isocat.org/datcat/DC-1232" type="simple">
		<dcif:administrationInformationSection>
			<dcif:administrationRecord>
				<dcif:identifier>adverb</dcif:identifier>
				<dcif:version>1:0</dcif:version>
				<dcif:registrationStatus>private</dcif:registrationStatus>
				<dcif:justification>ISO 12620;ISO 30042; morphosyntax</dcif:justification>
				<dcif:origin>ISO 12620;ISO 30042; morphosyntax</dcif:origin>
				<dcif:explanatoryComment xml:lang="en">This DC has been added to Terminology.</dcif:explanatoryComment>
				<dcif:effectiveDate>2012-06-08</dcif:effectiveDate>
				<dcif:creation>
					<dcif:creationDate>2004-07-09</dcif:creationDate>
					<dcif:changeDescription xml:lang="en">?</dcif:changeDescription>
				</dcif:creation>
				<dcif:lastChange>
					<dcif:lastChangeDate>2012-06-16</dcif:lastChangeDate>
					<dcif:changeDescription xml:lang="en">Corrected stylistic anomaly (see MAF and
						correct there as well.</dcif:changeDescription>
				</dcif:lastChange>
			</dcif:administrationRecord>
		</dcif:administrationInformationSection>
		<dcif:descriptionSection>
			<dcif:profile>Morphosyntax</dcif:profile>
			<dcif:profile>Terminology</dcif:profile>
			<dcif:dataElementNameSection>
				<dcif:dataElementName>adverb</dcif:dataElementName>
				<dcif:source>ISO 12620;ISO 30042; morphosyntax</dcif:source>
			</dcif:dataElementNameSection>
			<dcif:languageSection>
				<dcif:language>en</dcif:language>
				<dcif:nameSection>
					<dcif:name xml:lang="en">adverb</dcif:name>
					<dcif:nameStatus>standardized name</dcif:nameStatus>
				</dcif:nameSection>
				<dcif:definitionSection>
					<dcif:definition xml:lang="en">Part of speech used to refer to a heterogeneous
						group of words whose most frequent function is to specify the mode of action
						of the verb.</dcif:definition>
					<dcif:source>Based on Crystal 2003</dcif:source>
					<dcif:note xml:lang="en" />
				</dcif:definitionSection>
				<dcif:exampleSection>
					<dcif:example xml:lang="en"> She threw the ball far, She threw the ball very
						far, An extremely big horse </dcif:example>
					<dcif:source>
						www.sil.org/linguistics/GlossaryOfLinguisticTerms/WhatIsAnAdposition.htm </dcif:source>
				</dcif:exampleSection>
			</dcif:languageSection>
		</dcif:descriptionSection>
	</dcif:dataCategory>
